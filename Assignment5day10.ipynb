    "import time\n",
    "import calendar\n",
    "t1=\"15-08-2020 15:20:20\"\n",
    "def Human_epoch(t1):\n",
    "    t9=time.strptime(t1,\"%d-%m-%Y %H:%M:%S\")\n",
    "    t2=calendar.timegm(t9) #gmt timezone\n",
    "    t3=time.mktime(t9)     #local computer timezone\n",
    "    return t2,t3\n",
    "\n",
    "print(Human_epoch(t1))"
   ]
